<Window x:Class="CustomCommands.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CustomCommands"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Button Command="{Binding ClickCommand}">Execute</Button>
        <Button Click="ButtonLock_OnClick">Lock</Button>
        <Button Click="ButtonUnlock_OnClick">Unlock</Button>
    </StackPanel>
</Window>
